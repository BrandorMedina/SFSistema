@model IEnumerable<AdminLTE.Models.Docs>

@{
    ViewData["Title"] = "Index";
}
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table accent-primary">
    <thead>
        <tr>
            <th>
               USUARIOS
            </th>
            <th>
                TIPO DE DOCUMENTO
            </th>
            <th>
               ASUNTO
            </th>
            <th>
                FECHA DE CREACION
            </th>
            <th>
                FECHA A ENTREGAR
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Usuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tipo_documento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Asunto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Creado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Limite)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
